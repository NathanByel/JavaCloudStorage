<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.nbdev.cloud.client.LoginController">
    <children>
        <TextField fx:id="tfLogin" focusTraversable="false" promptText="Логин" />
        <TextField fx:id="tfPass" focusTraversable="false" promptText="Пароль">
            <VBox.margin>
                <Insets top="8.0" />
            </VBox.margin>
        </TextField>
        <Button fx:id="btnLogin" focusTraversable="false" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onLoginClick" text="Войти">
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </Button>
        <Button fx:id="btnSignUp" focusTraversable="false" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onRegistrationClick" text="Регистрация">
            <VBox.margin>
                <Insets top="40.0" />
            </VBox.margin>
        </Button>
    </children>
    <padding>
        <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
    </padding>
</VBox>
