<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.nbdev.cloud.client.MainController">
    <children>
        <HBox alignment="CENTER" VBox.vgrow="ALWAYS">
            <children>
            <VBox alignment="CENTER" spacing="8.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Локальные файлы">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font></Label>
                      <ListView fx:id="filesListLeft" focusTraversable="false" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
                <VBox alignment="CENTER" spacing="10.0">
                    <children>
                        <Button fx:id="btnMoveToRight" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="--&gt;" />
                        <Button fx:id="btnMoveToLeft" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="&lt;--" />
                        <Button fx:id="btnDelete" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Удалить" />
                    </children>
                    <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                    </HBox.margin>
                </VBox>
            <VBox alignment="CENTER" spacing="8.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Облако">
                     <font>
                        <Font name="System Bold" size="18.0" />
                     </font>
                  </Label>
                      <ListView fx:id="filesListRight" focusTraversable="false" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding>
        </HBox>
    </children>
</VBox>
